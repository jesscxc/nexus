import React, { useState, useEffect } from 'react';

const docPages = {
 top: <><p>I am the top page</p></>,
 "orb-world": <><p>Wanna know about the Orb World?</p></>,
 "orb-lap": <><p>What is Orb Lab?  Many have asked.  Few know.</p></>,
}



function Docs(props) {
  const [page, setPage] = useState(undefined)

  useEffect(() => {
    setPage(props.match.params.page)
  },[])
  
  return (
    <>
      <h1>{page}</h1>
      {docPageContent[page]}
    </>
  );
}

export default Docs;
